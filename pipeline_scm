def KUBECONFIG="--kubeconfig=/data_bck/kubeconfig/config"
env.KUBECTL_ARGS="--kubeconfig=/data_bck/kubeconfig/config"
pipeline {
  agent { label 'common' }
  stages {
    stage('Deploy') {
        steps {
            timeout(time: 1, unit: 'MINUTES') {sh "export KUBECONFIG=/data_bck/kubeconfig/config;kubectl apply -f stream.yaml;kubectl logs -f jenkins-stream-rsnfj -n kube-app"}
        }
    }    
  }  
}


